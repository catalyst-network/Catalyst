<Project Sdk="Microsoft.NET.Sdk">
    <PropertyGroup>
        <Title>Catalyst Redis Helper</Title>
        <Authors>James Kirkby (james.kirby@atlascity.io)</Authors>
        <PackageDescription>Redis Helper</PackageDescription>
        <Copyright>MIT</Copyright>
        <PackageRequireLicenseAcceptance>True</PackageRequireLicenseAcceptance>
        <PackageLicenseUrl>http://github.com/atlascity/catalyst-Redis/LICENSE.md</PackageLicenseUrl>
        <PackageProjectUrl>http://github.com/atlascity/catalyst-Redis/</PackageProjectUrl>
        <RepositoryUrl>http://github.com/atlascity/catalyst-Redis/</RepositoryUrl>
        <RepositoryType>git</RepositoryType>
        <AssemblyCompanyAttribute>AtlasCity.io</AssemblyCompanyAttribute>
        <AssemblyCopyrightAttribute>Copyright © 2019 AtlasCity.io</AssemblyCopyrightAttribute>
        <TargetFramework>netcoreapp2.1</TargetFramework>
        <RootNamespace>Catalyst.Helpers.Redis</RootNamespace>
        <PackageId>Catalyst.Helpers.Redis</PackageId>
        <Deterministic>true</Deterministic>
        <Optimize>true</Optimize>
        <PlatformTarget>AnyCPU</PlatformTarget>
        <RuntimeFrameworkVersion>2.1.1</RuntimeFrameworkVersion>
        <TargetLatestRuntimePatch>true</TargetLatestRuntimePatch>
        <GenerateAssemblyConfigurationAttribute>false</GenerateAssemblyConfigurationAttribute>
        <GenerateAssemblyCompanyAttribute>true</GenerateAssemblyCompanyAttribute>
        <GenerateAssemblyProductAttribute>false</GenerateAssemblyProductAttribute>
    </PropertyGroup>
    <ItemGroup>
        <PackageReference Include="Google.Protobuf" Version="3.6.1" />
        <PackageReference Include="Microsoft.Extensions.Configuration.Abstractions" Version="3.0.0-preview.18572.1" />
        <PackageReference Include="StackExchange.Redis" Version="2.0.513" />
        <PackageReference Include="StackExchange.Redis.Extensions.Core" Version="4.0.5" />
        <PackageReference Include="StackExchange.Redis.Extensions.Newtonsoft" Version="4.0.5" />
        <PackageReference Include="Dawn.Guard" Version="1.7.0" />
    </ItemGroup>
    <ItemGroup>
        <ProjectReference Include="..\Catalyst.Helpers.KeyValueStore\Catalyst.Helpers.KeyValueStore.csproj" />
    </ItemGroup>
    <PropertyGroup Condition="'$(Configuration)'=='Release'">
        <DebugType>none</DebugType>
        <DebugSymbols>False</DebugSymbols>
    </PropertyGroup>
    <ItemGroup>
        <Compile Remove="UnitTests\**" />
    </ItemGroup>
    <ItemGroup>
        <EmbeddedResource Remove="UnitTests\**" />
    </ItemGroup>
    <ItemGroup>
        <None Remove="UnitTests\**" />
    </ItemGroup>
    <PropertyGroup>
        <DefaultItemExcludes>./UnitTests</DefaultItemExcludes>
    </PropertyGroup>
</Project>