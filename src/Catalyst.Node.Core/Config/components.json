{
  "defaultAssembly": "Catalyst.Node.Core",
  "components": [
    {
      "type": "Catalyst.Node.Core.Components.Redis.RedisStore, Catalyst.Node.Core",
      "services": [
        {
          "type": "Catalyst.Node.Common.IKeyValueStore, Catalyst.Node.Common"
        }
      ]
    }
  ],
  //simply comment modules that you don't need.
  "modules": [
    {
      "type": "Catalyst.Node.Core.Modules.Gossip.GossipModule",
      "parameters": {
        "nameProvider": 1
      }
    },
    {
      "type": "Catalyst.Node.Core.Modules.Dfs.DfsModule",
      "parameters": {
        "connectRetries": 10,
        "apiPath": "api/v0/"
      }
    },
    { "type": "Catalyst.Node.Core.Modules.Consensus.ConsensusModule" },
    { "type": "Catalyst.Node.Core.Modules.Contract.ContractModule" },
    { "type": "Catalyst.Node.Core.Modules.Ledger.LedgerModule" },
    { "type": "Catalyst.Node.Core.Modules.Mempool.MempoolModule" }
  ] 
}
