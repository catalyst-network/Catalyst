<Project Sdk="Microsoft.NET.Sdk">
    <PropertyGroup>
        <Title>Catalyst Protocol Node Rpc Grpc Server</Title>
        <Authors>James Kirkby (james.kirby@atlascity.io)</Authors>
        <PackageDescription>Catalyst Node Rpc Grpc Server SDK</PackageDescription>
        <Copyright>MIT</Copyright>
        <PackageRequireLicenseAcceptance>True</PackageRequireLicenseAcceptance>
        <PackageLicenseUrl>http://github.com/atlascity/catalyst-protocol-node-rpc-grpc-server/LICENSE.md</PackageLicenseUrl>
        <PackageProjectUrl>http://github.com/atlascity/catalyst-protocol-node-rpc-grpc-server</PackageProjectUrl>
        <RepositoryUrl>http://github.com/atlascity/catalyst-protocol-node-rpc-grpc-server</RepositoryUrl>
        <RepositoryType>git</RepositoryType>
        <AssemblyCompanyAttribute>AtlasCity.io</AssemblyCompanyAttribute>
        <AssemblyCopyrightAttribute>Copyright © 2019 AtlasCity.io</AssemblyCopyrightAttribute>
        <TargetFramework>netcoreapp2.1</TargetFramework>
        <RootNamespace>Catalyst.Protocol.Node.Rpc.Grpc.Server</RootNamespace>
        <PackageId>Catalyst.Protocol.Node.Rpc.Grpc.Server</PackageId>
        <AssemblyName>Catalyst.Protocol.Node.Rpc.Grpc.Server</AssemblyName>
        <Deterministic>true</Deterministic>
        <Optimize>true</Optimize>
        <PlatformTarget>AnyCPU</PlatformTarget>
        <RuntimeFrameworkVersion>2.1.1</RuntimeFrameworkVersion>
        <TargetLatestRuntimePatch>true</TargetLatestRuntimePatch>
        <GenerateAssemblyConfigurationAttribute>false</GenerateAssemblyConfigurationAttribute>
        <GenerateAssemblyCompanyAttribute>true</GenerateAssemblyCompanyAttribute>
        <GenerateAssemblyProductAttribute>false</GenerateAssemblyProductAttribute>
    </PropertyGroup>
    <ItemGroup>
        <PackageReference Include="Grpc" Version="1.16.0" />
        <PackageReference Include="Grpc.Core" Version="1.16.0" />
        <PackageReference Include="Google.Protobuf" Version="3.6.1" />
    </ItemGroup>
    <ItemGroup>
        <ProjectReference Include="..\Catalyst.Protocol.Node.Rpc\csharp\Catalyst.Protocol.Node.Rpc.csproj" />
    </ItemGroup>
    <PropertyGroup Condition="'$(Configuration)'=='Release'">
        <DebugType>none</DebugType>
        <DebugSymbols>False</DebugSymbols>
    </PropertyGroup>
    <ItemGroup>
        <Compile Remove="UnitTests\**" />
    </ItemGroup>
    <ItemGroup>
        <EmbeddedResource Remove="UnitTests\**" />
    </ItemGroup>
    <ItemGroup>
        <None Remove="UnitTests\**" />
    </ItemGroup>
    <PropertyGroup>
        <DefaultItemExcludes>./UnitTests</DefaultItemExcludes>
    </PropertyGroup>
</Project>